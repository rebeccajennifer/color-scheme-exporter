{
  "$schema"     : "vscode://schemas/color-theme"

, "colors":
  { "foreground"                                          : "#FG__NORM"
  , "selection.background"                                : "#VIO_BOLD5f"
  , "errorForeground"                                     : "#FG__INVL"
  , "focusBorder"                                         : "#CYA_BOLD"

  , "editor.background"                                   : "#BG__NORM"
  , "editor.foreground"                                   : "#FG__NORM"
  , "editorCursor.background"                             : "#BG__NORM"
  , "editorCursor.foreground"                             : "#GRN_NORM"
  , "editor.selectionBackground"                          : "#BLU_BOLD5f"
  , "editor.selectionForeground"                          : "#BG__NORM"
  , "editorRuler.foreground"                              : "#FG__NORM0f"

  , "menu.background"                                    : "#BG__BOLD"
  //____________________________________________________________________
  // HIGHLIGHTING AND SELECTION
  //____________________________________________________________________
  , "editor.wordHighlightTextBackground"                  : "#VIO_BOLD5f"
  , "editor.wordHighlightBackground"                      : "#FG__NORM0f"
  , "editor.lineHighlightBackground"                      : "#BG__BOLD"

  , "editorLineNumber.activeForeground"                   : "#GRN_NORM"
  , "editorLineNumber.foreground"                         : "#GRY_NORM"

  , "editor.findMatchHighlightBackground"                 : "#BLU_BOLD7f"

  //____________________________________________________________________
  // PEEK VIEW - Displayed with right-click and "Go to *"
  //____________________________________________________________________
  , "peekViewEditor.background"                           : "#BLU_BOLD5f"
  , "peekViewEditor.matchHighlightBackground"             : "#VIO_BOLD5f"
  , "peekViewResult.matchHighlightBackground"             : "#VIO_BOLD5f"
  , "peekView.border"                                     : "#CYA_BOLD5f"
  , "peekViewTitleLabel.foreground"                       : "#FG__NORM"
  , "peekViewResult.fileForeground"                       : "#FG__NORM"
  , "peekViewResult.selectionBackground"                  : "#CYA_BOLD5f"
  , "peekViewResult.selectionForeground"                  : "#FG__NORM"

  // Peek View line number background
  //, "peekViewEditorGutter.background"                     :"#FG__NORM0f"

  //____________________________________________________________________
  // TERMINAL
  //____________________________________________________________________
  , "terminal.background"                                 : "#BG__NORM"
  , "terminal.foreground"                                 : "#FG__NORM"
  , "terminalCursor.background"                           : "#BG__NORM"
  , "terminalCursor.foreground"                           : "#GRN_NORM"
  //____________________________________________________________________
  , "terminal.ansiBlack"                                  : "#BLK_NORM"
  , "terminal.ansiRed"                                    : "#RED_NORM"
  , "terminal.ansiGreen"                                  : "#GRN_NORM"
  , "terminal.ansiYellow"                                 : "#YEL_NORM"
  , "terminal.ansiBlue"                                   : "#BLU_NORM"
  , "terminal.ansiMagenta"                                : "#VIO_NORM"
  , "terminal.ansiCyan"                                   : "#CYA_NORM"
  , "terminal.ansiWhite"                                  : "#WHT_NORM"
  //____________________________________________________________________
  , "terminal.ansiBrightBlack"                            : "#BLK_BOLD"
  , "terminal.ansiBrightRed"                              : "#RED_BOLD"
  , "terminal.ansiBrightGreen"                            : "#GRN_BOLD"
  , "terminal.ansiBrightYellow"                           : "#YEL_BOLD"
  , "terminal.ansiBrightBlue"                             : "#BLU_BOLD"
  , "terminal.ansiBrightMagenta"                          : "#VIO_BOLD"
  , "terminal.ansiBrightCyan"                             : "#CYA_BOLD"
  , "terminal.ansiBrightWhite"                            : "#WHT_BOLD"

  //____________________________________________________________________
  // BRACKETS
  //____________________________________________________________________
  , "editorBracketHighlight.foreground1"                  : "#GRN_BOLD"
  , "editorBracketHighlight.foreground3"                  : "#CYA_BOLD"
  , "editorBracketHighlight.foreground2"                  : "#RED_BOLD"
  , "editorBracketHighlight.unexpectedBracket.foreground" : "#FG__INVL"

  //____________________________________________________________________
  // DROPDOWN
  //____________________________________________________________________

  , "dropdown.background"                                 : "#FG__NORM0f"
  , "dropdown.foreground"                                 : "#FG__NORM"
  , "dropdown.border"                                     : "#FG__NORM0f"
  , "dropdown.listBackground"                             : "#FG__NORM0f"

  //____________________________________________________________________
  // TABS
  //____________________________________________________________________
  , "tab.activeBackground"                                : "#BG__NORM"
  , "tab.activeForeground"                                : "#FG__NORM"
  , "tab.inactiveBackground"                              : "#FG__NORM0f"
  , "tab.inactiveForeground"                              : "#GRY_NORM"
  , "tab.unfocusedActiveForeground"                       : "#BLU_BOLD7f"
  , "tab.unfocusedInactiveForeground"                     : "#GRY_NORM"

  //____________________________________________________________________
  // PANELS
  //____________________________________________________________________
  , "panel.background"                                    : "#FG__NORM0f"
  , "panel.border"                                        : "#FG__NORM0f"
  , "panelTitle.activeForeground"                         : "#FG__WEAKef"
  , "panelTitle.inactiveForeground"                       : "#FG__NORM"
  , "panelTitle.border"                                   : "#BG__NORM"
  //, "panelSectionHeader.background"                       : "#GRN_NORM"
  //, "panelTitleBadge.foreground"                          : "#BG__NORM"

  //____________________________________________________________________
  // STATUS BAR (BOTTOM OF WINDOW)
  //____________________________________________________________________
  , "statusBar.background"                                : "#BLU_BOLD7f"
  , "statusBar.foreground"                                : "#FG__NORM"
  , "statusBarItem.remoteBackground"                      : "#BLU_BOLD7f"
  , "statusBarItem.errorBackground"                       : "#BG__INVL"

  //____________________________________________________________________
  // ACTIVITY BAR (LEFT OF WINDOW)
  //____________________________________________________________________
  , "activityBar.foreground"                              : "#FG__NORMef"
  , "activityBar.background"                              : "#BG__BOLD"
  , "activityBar.activeBackground"                        : "#VIO_BOLD5f"
  , "activityBarBadge.background"                         : "#GRN_NORM"
  , "activityBarBadge.foreground"                         : "#BG__BOLD"
  , "icon.foreground"                                     : "#GRY_NORM"

  //____________________________________________________________________
  // SIDEBAR (PROJECT EXLPORER, ETC)
  //____________________________________________________________________
  , "sideBar.background"                                  : "#BG__NORM"
  , "sideBar.foreground"                                  : "#FG__NORM"
  , "sideBar.border"                                      : "#FG__NORM0f"
  , "sideBarTitle.background"                             : "#BG__NORM"
  , "sideBarTitle.foreground"                             : "#FG__NORM"
  , "sideBarSectionHeader.background"                     : "#FG__NORM0f"
  , "sideBarSectionHeader.foreground"                     : "#FG__NORM"

  , "list.activeSelectionBackground"                      : "#VIO_BOLD5f"
  , "list.activeSelectionForeground"                      : "#FG__NORM"
  , "list.inactiveSelectionBackground"                    : "#FG__NORM0f"
  , "list.inactiveSelectionForeground"                    : "#BLU_BOLD7f"

  , "list.hoverBackground"                                : "#FG__NORM0f"
  , "list.hoverForeground"                                : "#FG__NORM"

  // Drag and drop files
  , "list.dropBackground"                                 : "#GRN_BOLD"
  , "list.dropBetweenBackground"                          : "#GRN_BOLD"

  , "list.highlightForeground"                            : "#GRN_NORM"
  , "list.focusBackground"                                : "#BLU_BOLD7f"

  //____________________________________________________________________
  // BUTTONS
  //____________________________________________________________________
  , "button.background"                                   : "#FG__NORM0f"
  , "button.foreground"                                   : "#FG__NORM"
  , "button.hoverBackground"                              : "#VIO_BOLD5f"

  //____________________________________________________________________
  // DEBUG
  //____________________________________________________________________
  , "debugIcon.breakpointForeground"                      : "#FG__INVL"
  , "debugIcon.startForeground"                           : "#GRN_NORM"
  , "debugIcon.stopForeground"                            : "#RED_NORM"
  , "debugIcon.stepIntoForeground"                        : "#BLU_NORM"
  , "debugIcon.stepOverForeground"                        : "#BLU_NORM"
  , "debugIcon.stepOutForeground"                         : "#BLU_NORM"
  , "debugIcon.continueForeground"                        : "#VIO_NORM"
  , "debugIcon.pauseForeground"                           : "#VIO_NORM"
  , "debugIcon.restartForeground"                         : "#CYA_NORM"
  , "editor.inlineValuesBackground"                       : "#FG__NORM0f"
  , "editor.inlineValuesForeground"                       : "#YEL_NORM"
  , "editor.stackFrameHighlightBackground"                : "#FG__NORM0f"
  , "statusBar.debuggingBackground"                       : "#YEL_NORM"
  , "statusBar.debuggingForeground"                       : "#BG__NORM"
  , "statusBar.noFolderBackground"                       : "#VIO_BOLD5f"

  //____________________________________________________________________
  // SOURCE CONTROL
  //____________________________________________________________________
  , "gitDecoration.addedResourceForeground"               : "#GRN_BOLD"
  , "gitDecoration.modifiedResourceForeground"            : "#YEL_NORM"
  , "gitDecoration.untrackedResourceForeground"           : "#VIO_NORM"
  , "gitDecoration.deletedResourceForeground"             : "#FG__INVL"

  , "editorGutter.addedBackground"                        : "#GRN_NORM"
  , "editorGutter.modifiedBackground"                     : "#YEL_NORM"
  , "editorGutter.deletedBackground"                      : "#FG__INVL"

  , "scmGraph.foreground1"                                : "#YEL_NORM"
  , "scmGraph.historyItemBaseRefColor"                    : "#RED_BOLD"
  , "scmGraph.historyItemRefColor"                        : "#CYA_BOLD"
  , "scmGraph.historyItemRemoteRefColor"                  : "#RED_BOLD"

  , "diffEditor.insertedLineBackground"                   : "#GRN_BOLD1f"
  , "diffEditor.insertedTextBackground"                   : "#GRN_BOLD5f"

  , "diffEditor.removedLineBackground"                    : "#FG__INVL1f"
  , "diffEditor.removedTextBackground"                    : "#BG__INVL5f"

  //, "scmGraph.historyItemHoverDefaultLabelBackground"     : "#GRN_BOLD"
  //, "scmGraph.historyItemHoverDefaultLabelForeground"     : "#FG__NORM"
  //, "scmGraph.foreground2"                                : "#RED_NORM"
  //, "scmGraph.foreground3"                                : "#RED_NORM"
  //, "scmGraph.foreground4"                                : "#RED_NORM"
  //, "scmGraph.foreground5"                                : "#RED_NORM"

  //____________________________________________________________________
  // MINIMAP
  //____________________________________________________________________
  , "minimap.background"                                  : "#FG__NORM0f"
  , "minimapGutter.addedBackground"                       : "#GRN_NORM"
  , "minimapGutter.deletedBackground"                     : "#FG__INVL"
  , "minimapGutter.modifiedBackground"                    : "#YEL_NORM"

  //____________________________________________________________________
  // ERRORS AND WARNINGS
  //____________________________________________________________________
  , "editorWarning.foreground"                            : "#YEL_BOLD"
  , "editorError.foreground"                              : "#FG__INVL"

  , "list.warningForeground"                              : "#YEL_NORM"
  , "list.errorForeground"                                : "#FG__INVL"

  , "minimap.errorHighlight"                              : "#FG__INVL"
  , "minimap.warningHighlight"                            : "#YEL_NORM"
  , "minimap.selectionHighlight"                          : "#CYA_NORMaf"

  , "inputValidation.warningBackground"                   : "#FG__INVL"
  , "inputValidation.warningForeground"                   : "#FG__NORM"
  , "inputValidation.warningBorder"                       : "#FG__NORM"
  , "inputValidation.errorBackground"                     : "#BG__NORM"
  , "inputValidation.errorForeground"                     : "#FG__INVL"
  , "inputValidation.errorBorder"                         : "#FG__INVL"

  , "problemsErrorIcon.foreground"                        : "#FG__INVL"
  , "problemsWarningIcon.foreground"                      : "#YEL_NORM"
  , "problemsInfoIcon.foreground"                         : "#YEL_NORM"

  //____________________________________________________________________
  // ICONS
  //____________________________________________________________________
  , "editorLightBulb.foreground"                          : "#YEL_NORM"

  , "symbolIcon.arrayForeground"                          : "#GRN_BOLD"
  , "symbolIcon.booleanForeground"                        : "#FG__NORM"
  , "symbolIcon.classForeground"                          : "#CYA_NORM"
  //, "symbolIcon.colorForeground"                          : "#"
  , "symbolIcon.constantForeground"                       : "#YEL_NORM"
  , "symbolIcon.constructorForeground"                    : "#VIO_NORM"
  , "symbolIcon.enumeratorForeground"                     : "#VIO_NORM"
  , "symbolIcon.enumeratorMemberForeground"               : "#YEL_NORM"
  //, "symbolIcon.eventForeground"                          : "#"
  , "symbolIcon.fieldForeground"                          : "#BLU_NORM"
  //, "symbolIcon.fileForeground"                           : "#"
  //, "symbolIcon.folderForeground"                         : "#"
  , "symbolIcon.functionForeground"                       : "#CYA_BOLD"
  //, "symbolIcon.interfaceForeground"                      : "#"
  //, "symbolIcon.keyForeground"                            : "#"
  //, "symbolIcon.keywordForeground"                        : "#"
  , "symbolIcon.methodForeground"                         : "#CYA_BOLD"
  , "symbolIcon.moduleForeground"                         : "#YEL_NORM"
  , "symbolIcon.namespaceForeground"                      : "#VIO_NORM"
  //, "symbolIcon.nullForeground"                           : "#"
  //, "symbolIcon.numberForeground"                         : "#"
  //, "symbolIcon.objectForeground"                         : "#"
  //, "symbolIcon.operatorForeground"                       : "#"
  //, "symbolIcon.packageForeground"                        : "#"
  , "symbolIcon.propertyForeground"                       : "#BLU_NORM"
  //, "symbolIcon.referenceForeground"                      : "#"
  //, "symbolIcon.snippetForeground"                        : "#"
  , "symbolIcon.stringForeground"                         : "#VIO_BOLD"
  , "symbolIcon.structForeground"                         : "#YEL_NORM"
  , "symbolIcon.textForeground"                           : "#VIO_BOLD"
  //, "symbolIcon.typeParameterForeground"                  : "#"
  , "symbolIcon.unitForeground"                           : "#RED_NORM"
  , "symbolIcon.variableForeground"                       : "#BLU_NORM"

  //____________________________________________________________________
  // MISC
  //____________________________________________________________________

  }

, "tokenColors":
  [ {}

  , { "name"  : "Foreground"
    , "scope": [ ""
      , "source"
      , "text.html" // Comment specifier
      , "variable.parameter"
      , "meta.body.function.definition"
      , "meta.function.definition"
      , "meta.function-call.arguments"
      , "punctuation"
      , "punctuation.definition.string"
      , "punctuation.definition.string.begin.python"
      , "punctuation.support.type.property-name"
      , "punctuation.separator.arguments"
      , "punctuation.separator.array"
      , "punctuation.separator.dictionary.pair"
      , "punctuation.separator.key-value"             // class="value"
      , "string.interpolated"
      , ""
      ], "settings": { "foreground": "#FG__NORM" }
  }

  , { "name"  : "Foreground Bold"
    , "scope": [ ""
      , "keyword.control.ternary" // Java - : in Circle: "circle"
      , ""
      ], "settings": { "foreground": "#FG__WEAKef" }
    }

  , { "name"  : "Color 1: Pink"
    , "scope" : [ ""
      , "entity.name.function.preprocessor"
      , "entity.name.operator"
      , "heading.1.markdown"
      , "heading.1.quarto"
      , "keyword.control"               // switch, case, break, if, for
      , "keyword.other.directive.using" // using a = float
      , "keyword.other.operator"
      , "keyword.other.using"
      , ""
      ] , "settings": { "foreground": "#RED_NORM" }
    }

  , { "name"  : "Color 9: Pink Bold"
    , "scope" : [ ""
      , "invalid"
      , ""
      ], "settings": { "foreground": "#FG__INVL" }
    }

  , { "name"  : "Color 2: Green"
    , "scope" : [ ""

      , "heading.3.markdown"
      , "heading.3.quarto"
      , "keyword.operator"
      , "keyword.operator.assignment"
      , "keyword.operator.comparison"
      , "punctuation.definition.constant"         // # in #ff0000
      , "punctuation.definition.link.description"
      , "punctuation.separator.annotation.result" // -> in def f() -> int:
      ]
    , "settings": { "foreground": "#GRN_NORM" }
    }

  , { "name"  : "Color 10: Green Bold"
    , "scope" : [ ""
      , "keyword.control.directive"
      , "keyword.control.import"
      , "keyword.other.export"
      , "keyword.other.import"
      , "storage.type.modifier"        // public
      , ""
      ]
    , "settings": { "foreground": "#GRN_BOLD"}
    }

  , { "name"  : "Color 3: Yellow"
    , "scope" : [ ""
      , "constant.language"
      , "heading.2.markdown"
      , "heading.2.quarto"
      , "keyword"
      , "keyword.operator"
      , "keyword.operator.cast"                       // static_cast
      , "keyword.operator.delete"
      , "keyword.operator.new"
      , "meta.separator"
      , "punctuation.separator.continuation.line"
      , "storage.modifier"                            // const, static
      , "support.constant.font-name"
      , ""
      ], "settings": { "foreground": "#YEL_NORM"}
    }

  , { "name": "Color 11: Yellow Bold"
    , "scope": [""
      , "constant.character.escape"    // \n in "\n"
      , "constant.other.placeholder"   // %02X in string
      , "storage.type.format"          // 2x in {a:2x}

      , "entity.name.operator.custom-literal"
      , "keyword.other.unit"
      , "punctuation.separator.continuation.line"
      ], "settings": { "foreground": "#YEL_BOLD" }
    }

 , { "name"  : "Color 4: Blue"
    , "scope" : [ ""
      //, "entity.name.variable"
      , "heading.4.markdown"
      , "heading.4.quarto"
      , "meta.member.access.python"
      , "markup.list.numbered"
      , "variable.other.object"
      , "variable.other.property" // b in a.b
      , "support.type.property-name"
      , ""
      ], "settings": { "foreground": "#BLU_NORM"}
    }

  , { "name"  : "Color 12: Blue Bold"
    , "scope" : [ ""
      , "constant.other.placeholder"
      , "entity.name.tag"
      , "entity.name.type.parameter"
      , "markup.underline.link.image"
      , "meta.definition.variable.name"
      , "meta.method.return-type"
      , "support.constant.color"                    // red
      , "support.constant.property-value"           // css none
      , "support.type"
      , "storage.type.built-in"                     // int
      , "storage.type.function"                     // def in python
      , "storage.type.primitive"                    // int
      , "storage.type.string"                       // f in f'string'
      , "storage.type.template.argument.typename"   // public
      , "support.variable"
      , ""
      ], "settings": { "foreground": "#BLU_BOLD" }
    }

  , { "name"  : "Color 5: Violet"
    , "scope": [ ""
      , "entity.name.scope-resolution"
      , "heading.5.markdown"
      , "heading.5.quarto"
      , "storage.type.class"
      , "storage.type.enum"
      , "storage.type.namespace.definition"
      , "storage.type.struct"
      , "storage.type.template"
      , "storage.type.enum.enum-key.class.cpp"
      , "variable.language.special.self"          // python
      , "variable.language.this"                  // java
      , "variable.other.constant"
      , "entity.name.namespace"
      , "keyword.control.import.shortcode"
      , ""
      ], "settings": { "foreground": "#VIO_NORM" }
    }

  , { "name"  : "Color 13: Violet Bold"
    , "scope" : [ ""
      , "constant.numeric"
      , "constant.other.color.rgb-value"
      , "constant.other.rgb-value"
      , "string"
      , ""
      ], "settings": { "foreground": "#VIO_BOLD"}
    }

  , { "name"  : "Color 6: Cyan"
    , "scope" : [ ""
      , "entity.name.type"
      , "entity.other"
      ]
    , "settings": { "foreground": "#CYA_NORM" }
    }

  , { "name"  : "Color 14: Cyan Bold"
    , "scope" : [ ""
      , "support.function"
      , "entity.name.function"
      , "meta.function-call"

      ], "settings": { "foreground": "#CYA_BOLD" }
    }

  , { "name"  : "Color 0: Grey"
    , "scope": [ ""
      , "comment"
      , "punctuation.definition.comment" // Comment specifier e.g. //
      , ""
      ], "settings": { "foreground": "#GRY_NORM" }
    }

  , { "name"  : "CSS property value"
    , "scope" : [ ""
      , "support.constant.media-type"
      , "support.constant.media"
      ]
    , "settings": { "foreground": "#BLU_BOLD" }
    }

  , { "name"  : "Regular expression groups"
    , "scope" : [ ""
      , "punctuation.definition.group.regexp"
      , "punctuation.definition.group.assertion.regexp"
      , "punctuation.definition.character-class.regexp"
      , "punctuation.character.set.begin.regexp"
      , "punctuation.character.set.end.regexp"
      , "keyword.operator.negation.regexp"
      , "support.other.parenthesis.regexp"
      ]
    , "settings": { "foreground": "#d75fd7" }
    }

  , { "scope": [
        "constant.character.character-class.regexp"
      , "constant.other.character-class.set.regexp"
      , "constant.other.character-class.regexp"
      , "constant.character.set.regexp"
      ]
    , "settings": { "foreground": "#RED_NORM" }
    }
  ]

  // These properties override tokenColors
, "semanticTokenColors":
  {
    "enumMember"    : "#YEL_NORM" // Enum member variables
  //, "variable"      : "#BLU_NORM" // Local variables
  //, "property"      : "#BLU_NORM" // Class member variables
  //, "parameter"     : "#BLU_NORM" // Function parameters
  //, "enumMember"    : "#BLU_NORM" // Class member variables
  //, "macro"         : "#WHT_BOLD" // PI in #define PI
  //, "type"          : "#BLU_NORM "// Don't set this
  }


}

